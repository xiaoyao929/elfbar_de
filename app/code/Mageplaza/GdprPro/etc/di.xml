            <argument name="collections" xsi:type="array">
                <item name="mageplaza_gdpr_delete_account_log_listing_data_source" xsi:type="string">Mageplaza\GdprPro\Model\ResourceModel\DeleteAccountLogs\Grid\Collection</item>
